{
    "name": "ng2-webpack-starter",
    "version": "0.0.1",
    "description": "Boilerplate for starting a project with Angular2 and Webpack",
    "repository": {
        "type": "git",
        "url": "https://github.com/drewwyatt/ng2-webpack-starter"
    },
    "scripts": {
        "start": "npm run dev-server:hot",
        "test": "./node_modules/.bin/karma start",
        "build": "./node_modules/.bin/webpack",
        "dev-server": "./node_modules/.bin/webpack-dev-server --inline --progress --profile --colors --watch --display-error-details --display-cached --content-base dist/",
        "dev-server:hot": "./node_modules/.bin/webpack-dev-server --inline --progress --profile --colors --watch --display-error-details --display-cached --content-base dist/ --hot",
        "typings": "./node_modules/.bin/typings",
        "postinstall": "./node_modules/.bin/typings install"
    },
    "author": "Drew Wyatt <drew.j.wyatt@gmail.com>",
    "dependencies": {
        "angular2": "^2.0.0-beta.14",
        "es6-shim": "^0.35.0",
        "reflect-metadata": "^0.1.2",
        "rxjs": "^5.0.0-beta.2",
        "zone.js": "^0.6.6"
    },
    "devDependencies": {
        "html-webpack-plugin": "^2.14.0",
        "jasmine-core": "2.4.1",
        "karma": "^0.13.22",
        "karma-chrome-launcher": "^0.2.3",
        "karma-cli": "^0.1.2",
        "karma-jasmine": "^0.3.8",
        "karma-phantomjs-launcher": "^1.0.0",
        "karma-sourcemap-loader": "^0.3.7",
        "karma-webpack": "^1.7.0",
        "phantomjs-prebuilt": "^2.1.7",
        "raw-loader": "^0.5.1",
        "ts-loader": "^0.8.1",
        "typescript": "^1.8.9",
        "typings": "^0.7.9",
        "webpack": "^1.12.14",
        "webpack-dev-server": "^1.14.1"
    },
    "engines": {
        "node": ">=5.0.0",
        "npm": "3.3.6"
    }
}
